<style>
 .typst-dom-page {
     margin:  0 auto 0 auto;
 }
 .article-header {
     text-align: center;
     width: clamp(360px, 65%, 750px);
     margin: 50px auto 50px auto;
 }
</style>
<article>
    <div class="article-header">
        <h2><a href="/<%= page.path %>"><%= page.title %></a></h2>
        <time><%= page.date.toISOString().slice(0, 10) %></time>
    </div>
    <div>
        <%- page.content %>
    </div>
</article>

